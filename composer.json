{
  "name": "jord-jd/php-camel-caser",
  "description": "Lets you use built-in PHP functions in camel case",
  "type": "library",
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    },
    {
      "name": "Jan-Marten de Boer",
      "email": "github@johmanx.com"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-json": ">=1.5"
  },
  "require-dev": {
    "kint-php/kint": "^2.2",
    "phpunit/phpunit": "^9.6"
  },
  "autoload": {
    "psr-4": {
      "JordJD\\CamelCaser\\": "src"
    },
    "files": [
      "autoload.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "JordJD\\CamelCaser\\Tests\\": "tests"
    },
    "files": [
      "tests/Fixture/functions.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  },
  "archive": {
    "exclude": [
      "/.gitignore",
      "/phpunit.xml",
      "/phpmd.xml",
      "/phpstan.neon",
      "/phpcs.xml",
      "/tests"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "replace": {
    "divineomega/php-camel-caser": "self.version"
  }
}
